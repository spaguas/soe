        iconLayersControl.on('activelayerchange', function(e) {
            console.log('layer switched', e.layer);
        });

        //$('#modal-filtro').modal('show');
        const modal_filtro = new bootstrap.Modal('#modal-filtro');

        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            maxZoom: 19,
            attribution: '© OpenStreetMap'
        }).addTo(map);

        let ugrhiLayer = L.layerGroup([]);
        let municipioLayer = L.layerGroup([]);
        let geo_distance_groups = L.layerGroup([]);

        municipioLayer.addTo(map);
        ugrhiLayer.addTo(map);
        geo_distance_groups.addTo(map);

        populateMunicipiosSelect();
        populateUgrhisSelect();
        fillFormWithUrl();

        $("#btn_filter").prop("disabled", true);

        let rst = '<%- outorgas %>';

        var markers = L.markerClusterGroup();

        let outorgas_table_obj = [];
        
        var hash = new L.Hash(map);

        let outorgas;

        $("#sol_req_num_cod").on('keyup', function(e){
          let value = $("#sol_req_num_cod").val();

          if(value.length > 0){
            $("#municipio,#cod_ugrhi,#finalidade,#tipo_uso,#data_publicacao").prop( "disabled", true );
            $("#btn_filter").prop("disabled", false);
          }
          else{
            $("#municipio,#cod_ugrhi,#finalidade,#tipo_uso,#data_publicacao").prop( "disabled", false );
          }
        });
        
        $("#cod_ugrhi,#municipio,#diretoria,#finalidade,#tipo_uso,#data_publicacao").on('change', function(e){
          //console.log("Select Ugrhi or Municipio Changed: ", $(e.currentTarget));
          let el = $(this);
          let current_id = el.attr('id');
          let current_value = el.find(":selected").val();

          //console.log("Element: ", el.attr('id'))
          if($("#cod_ugrhi").val() == "-" && $("#municipio").val() == "-" && ("#diretoria").val() == "-"){
            $("#municipio,#cod_ugrhi,#diretoria").prop("disabled", false);
            $("#btn_filter").prop("disabled", true);
          }
          else if(current_id == 'municipio' && current_value != ''){
            //Disable Ugrhi
            $("#cod_ugrhi").prop("disabled", true);
            $("#diretoria").prop("disabled", true);
            $("#btn_filter").prop("disabled", false);
          }
          else if(current_id = 'ugrhi' && current_value != ''){
            $("#municipio").prop("disabled", true);
            $("#diretoria").prop("disabled", true);
            $("#btn_filter").prop("disabled", false);
          }
          else if(current_id = 'diretoria' && current_value != ''){
            $("#municipio").prop("disabled", true);
            $("#cod_ugrhi").prop("disabled", true);
            $("#btn_filter").prop("disabled", false);
          }

          //console.log("Element: ", el.find(":selected").val() );
        })

        //Check if any filter applyed to disable or enable filter button
        

        if(rst != ""){
          outorgas = JSON.parse(rst.replaceAll("&#34;","\""));

          outorgas.forEach(element => {
            let source = element._source;
            
            console.log("Source: ", source);

            let discharges = [];

            let months = ['jan','fev','mar','abr','mai','jun','jul','ago','set','out','nov','dez']

            //Calculate discharge 12 months
            for(let i = 0; i < months.length; i++){
              
              //let x = (""+i).padStart(2,'0');
              let x = months[i];
              let days  = source["days_in_month_"+x];
              let hours = source["hours_in_day_"+x];
              let mins  = source["mins_in_hours_"+x];
              let vaz   = source["vaz_"+x];
              let vol   = source["total_vol_m3_"+x];

              days = (days != '' && days != 'null' && days != null) ? days : 0;
              hours = (hours != '' && hours != 'null' && hours != null) ? hours : 0;
              mins = (mins != '' && mins != 'null' && mins != null) ? mins : 0;
              vaz = (vaz != '' && vaz != 'null' && vaz != null) ? vaz : 0;
              vol = (vol != '' && vol != 'null' && vol != null) ? vol : 0;

              //let dis   = days * (hours + (mins/60)) * vaz;
              let dis = vol;

              discharges.push({
                dias: days,
                horas: hours,
                minutos: mins,
                vazao: vaz,
                volume: dis
              });

              //console.log("Discharge: ", dis," - [days: ",days,", hours: ",hours,", minutes: ",mins,"]");
            }

            element._source["vazoes"] = discharges;
            element._source["volume_anual"] = _.sumBy(discharges, 'volume');
            element._source["vazao_max"] = _.maxBy(discharges, 'vazao');

            renderRowToTable("#tabela-outorgas", element._source);

            let icon_options = getIconOptions(element._source);

            let marker = L.marker([element._source.latitude, element._source.longitude],{
              title: element._source.uso_id,
              data: element,
              icon: L.BeautifyIcon.icon(icon_options)
            }).on('click', function(e){
              console.log("Clicou no marker: ", e);

              let opts = e.sourceTarget.options.data;

              const md = new bootstrap.Modal('#infoOutorga', {
                keyboard: false
              });

              md.show();            
              
              $("#infoOutorga").find(".modal-title").html("Requerimento: "+element._source.sol_req_num_cod+" - Código do Uso: "+element._source.uso_id);
              $("#infoOutorga").find(".modal-body").find(".table-vazoes").empty();

              $(".info-municipio").html(opts._source.municipio);

              if(opts._source.cnpj_requerente != null && opts._source.cnpj_requerente != undefined){
                $(".info-requerente").html(opts._source.cnpj_requerente + " - "+opts._source.razao_social_requerente);
              }
              else{
                $(".info-requerente").html("******");
              }
              
              if(opts._source.nome_curso_agua != null && opts._source.nome_curso_agua != undefined && opts._source.nome_curso_agua != ""){
                $(".info-curso-agua").html("Nome: "+opts._source.nome_curso_agua+" Dominio: "+opts._source.dominio+" - CNARH: "+opts._source.cnarh);
              }
              else{
                $(".info-curso-agua").html("-");
              }

              $(".info-curso-agua").html(opts._source.nome_curso_agua);

              $(".info-diretoria").html(opts._source.diretoria_nome);
              $(".info-ugrhi").html(getUgrhi(opts._source.cod_ugrhi));
              $(".info-coordenadas").html(truncateString(""+opts._source.latitude, 12)+","+truncateString(""+opts._source.longitude,12));

              $(".info-tipo").html(opts._source.tipo_uso);
              $(".info-sub-tipo").html(opts._source.subtipo_uso);
              $(".info-finalidade").html(opts._source.tipo_finalidade_grupo);
              $(".info-decisao").html(opts._source.decisao_pto);
              $(".info-processo").html(opts._source.processo);
              $(".info-portaria").html(opts._source.numero_portaria);

              if(opts._source.data_publicacao){
                $(".info-data-publicacao").html(opts._source.data_publicacao.substring(0,10));
                let dt_vencimento = moment(opts._source.data_publicacao.substring(0,10),'YYYY-MM-DD').add(opts._source.validade_meses_pto,'months');
                
                console.log("Data de Vencimento: ", dt_vencimento)
                //console.log("Data: ", moment(opts._source.data_publicacao.substring(0,10)).format("YYYY-MM-DD")," => ",opts._source.validade_pto_meses," <> ",dt_vencimento.format("YYYY-MM-DD"));
                if(dt_vencimento.format("YYYY-MM-DD") != opts._source.data_publicacao.substring(0,10)){
                  $(".info-validade-pto").html(dt_vencimento.format("YYYY-MM-DD"));
                }
              }
              else{
                $(".info-data-publicacao").html("-");
              }

              //$(".info-sazonalidade").html((opts._source.sazonalidade.indexOf("SIM ") > -1)?"Não":"Sim");
              $(".info-volume-anual").html(opts._source.volume_anual.toFixed(2));
              $(".info-vazao-max").html(opts._source.vazao_max.vazao);

              $(".info-location").html("<a target='_blank' href='https://maps.google.com?q="+opts._source.location+"'>Ver no Google Maps</a>");

              if(element._source.vazoes[0].vazao == null){
                $("#nav-discharges-tab").addClass("disabled");
              }
              else{
                $("#nav-discharges-tab").removeClass("disabled");
              }

              var total_volume = 0;

              $.each(element._source.vazoes, function(k,v){
                
                let line = ""
                if(v.vazao != null){
                  console.log("Vazão: ", v);
                  total_volume += v.volume;

                  line += "<tr>";
                    line += "<td class='scope'>"+(k+1)+"</td>";
                    line += "<td class='scope'><b>"+getMonthName(k+1)+"</b></td>";
                    line += "<td class='scope'>"+v.dias+"</td>";
                    line += "<td class='scope'>"+(v.horas).toLocaleString()+"</td>";
                    line += "<td class='scope'>"+(v.minutos).toLocaleString()+"</td>";
                    line += "<td class='scope'>"+(v.vazao).toLocaleString()+"</td>";
                    line += "<td class='scope'>"+(v.volume).toLocaleString()+"</td>";
                  line += "</tr>";
                }

                $("#infoOutorga").find(".modal-body").find(".table-vazoes").append(line);
              });
              
              let line = ""

              line += "<tr>";
                line += "<td class='scope' style='text-align: center' colspan='6'><b>Volume Anual (m³)</b></td>";
                line += "<td class='scope'>"+total_volume.toLocaleString()+"</td>";
              line += "</tr>";

              $("#infoOutorga").find(".modal-body").find(".table-vazoes").append(line);              
            });
            //console.log(marker);
            //Append Data to Table
            markers.addLayer(marker);
            
            //console.log("Bounds to element: ", element._source);
            bounds.extend([element._source.latitude, element._source.longitude]);
            
          });

          map.addLayer(markers);
          
          loadUgrhiLayer();
          loadMunicipioLayer();
          
          const panelRight = L.control.sidepanel('panel-legend', {
            panelPosition: 'right',
            hasTabs: true,
            tabsPosition: 'left',
            pushControls: true,
            darkMode: true
          }).addTo(map);

          if(bounds.isValid()){
            
