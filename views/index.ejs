          else if(name == "Diretoria da Bacia do Médio Tietê"){
            return "BMT";
          }
          else if(name == "Diretoria de Bacia do Baixo Tietê"){
            return "BBT";
          }
          else if(name == "Diretoria de Bacia do Peixe Paranapanema"){
            return "BPP";
          }
          else if(name == "Diretoria de Bacia do Turvo Grande"){
            return "BTG";
          }
          else if(name == "Diretoria de Bacia do Pardo Grande"){
            return "BPG";
          }
          else if(name == "Diretoria de Bacia do Ribeira de Iguape e Litoral Sul"){
            return "BRI-LS";
          }
          else if(name == "Diretoria de Bacia do Paraíba e Litoral Norte"){
            return "BP-LN";
          }
          else{
            return "Desconhecida";
          }
