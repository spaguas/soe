        $("#btn_filter_geo").on('click', function(e){
          console.log("Filter Geo Clicked...");
          $("#form_geo").submit();
        });
        
        function getIconClassByTipoUso(tipo_uso){
          let icon;

          switch(tipo_uso){
            case "Retificação / Revogação":
              icon = "fa-solid fa-plus text-info";
              break;
            case "Captação Subterrânea":
              icon = "fa-solid fa-arrow-up text-danger";
              break;
            case "Captação Superficial":
              icon = "fa-solid fa-arrow-up text-orange";
              break;
            case "Lançamento Superficial":
              icon = "fa-solid fa-arrow-down text-teal";
              break;
            case "Reservatório de Acumulação":
              icon = "fa-solid fa-chart-line text-cyan";
              break;
            case "Canalização":
              icon = "fa-solid fa-timeline text-primary";
              break;
            case "Travessia Aérea":
              icon = "fa-solid fa-dungeon text-primary";
              break;
            case "Travessia Subterrânea":
              icon = "fa-solid fa-dungeon text-warning";
              break;
            case "Desassoreamento e Limpeza de Margem":
              icon = "fa-solid fa-snowplow text-secondary";
              break;
            case "Travessia":
              icon = "fa-solid fa-bridge-water text-green";
              break;
            case "Poço Tubular":
              icon = "fa-solid fa-tower-observation text-orange";
              break;
            case "Obra/Serviço":
              icon = "fa-solid fa-screwdriver-wrench text-red";
              break;
            case "Desassoreamento":
              icon = "fa-solid fa-arrows-down-to-line text-purple";
              break;
            default:
              icon = "fa-solid fa-users text-danger";
          }

          return icon;
        }

        function getIconClassByFinality(finalidade){
          let finalidade_icon;

          switch(finalidade){
            case "Irrigação":
              finalidade_icon = "fa-solid fa-leaf text-info";
              break;
            case "Urbano":
              finalidade_icon = "fa-solid fa-building text-pink";
              break;
            case "Industrial":
              finalidade_icon = "fa-solid fa-industry text-warning";
              break;
            case "Doméstico":
              finalidade_icon = "fa-solid fa-home-user text-teal"
              break;
            case "Aquicultura":
              finalidade_icon = "fa-solid fa-fish text-primary" ;
              break;
            case "Comércio e Serviços":
              finalidade_icon = "fa-solid fa-cash-register text-orange";
              break;
