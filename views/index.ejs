        function fillFormWithUrl(){
          console.log(window.location.search);
          let params = new URLSearchParams(window.location.search);
          console.log(params);
          //let q = params.get("municipio");
          //console.log(q)

          if(window.location.search==''){
            modal_filtro.show();
          }

          $("#nro_requerimento").val(params.get("nro_requerimento")).change();
          $("#municipio").val(params.get("cod_ibge")).change();
          $("#ugrhi").val(params.get("ugrhi")).change();
          $('[name="data_publicacao_lte"]').val(params.get("data_publicacao_lte"));
          $('[name="data_publicacao_gte"]').val(params.get("data_publicacao_gte"));
          $("#tipo_uso").val(params.get("tp_uso_subtipo1"));
          $("#finalidade").val(params.get("finalidade_1630"));

          if(params.get("location")){
            let location = params.get("location").replaceAll("POINT ","").replaceAll("(","").replaceAll(")","").split(" ");
            console.log("Location: ", location);
            $("#latitude").val(location[1]);
            $("#longitude").val(location[0]);
          }
          
          $("#distance").val(params.get("distance"));

          changeRaioText("#txt_raio_km", params.get("distance"));
          updateLocationValue();


        }

        function updateLocationValue(){
          let lat = $("#latitude").val();
          let lon = $("#longitude").val();

          if(lat != '' && lon != ''){
            $("#location").val("POINT ("+lon+" "+lat+")");

            let distance_in_meters = parseFloat($("#distance").val())*1000;
            geo_distance_groups.clearLayers();
  
            let circle = L.circle([lat,lon],{radius: distance_in_meters});
            let marker = L.marker([lat,lon],{           
              icon: L.BeautifyIcon.icon({
                iconShape: 'circle-dot',
                borderWidth: 5,
                borderColor: 'blue'
              })
            });
            geo_distance_groups.addLayer(circle);
            geo_distance_groups.addLayer(marker);
  
            map.fitBounds(circle.getBounds());
          }          
        }

        function changeRaioText(target,value){
          const output = document.querySelector(""+target);
          output.textContent = value;
        }

        function renderCustomInfo(){
          
          let el = '<h4>Outorgas Carregadas</h4></br>'

          el += "<table width=100% bgcolor='#ffffff' class='table table-bordered'>";
          el += "<thead class=\"table-secondary\"><th></th><th>Total</th><th>"+outorgas.length+"</th></thead>";
          $.each(_.countBy(outorgas, function(o){ return o._source.finalidade_1630 }), function(k,v){
            //console.log(k," => ",v);
            el += '<tr><td><i class="'+getIconClassByFinalidade(k)+'"></i></td><td><b>'+k+'</b></td><td>'+v+"</td></tr>";
          });

          el += "</table>";

          $("#tab-legenda").html(el);

          console.log(outorgas);

          let ranking_outorgas = _.sortBy(outorgas, function(o){ return o._source.volume_anual });

          //console.log("Ranking: ", ranking_outorgas);

          let elx = "<table bgcolor='#ffffff' class='table table-bordered table-hover'>";
            elx += "<thead><th>#</th><th>Uso</th><th>Vol.Anual(m³)</th><thead>";
            elx += "<tbody>";
              $.each(_.reverse(ranking_outorgas), function(k,v){
                //console.log("Each: ", v)
                if(k < 10){
                  elx += "<tr onclick='map.flyTo(["+v._source.location+"],17)'><td>"+(k+1)+"</td><td>"+v._source.uso_id+"</td><td>"+(v._source.volume_anual).toLocaleString()+"</td></tr>";
                }
              });
            elx += "</tbody>";
          elx += "</table>";

          $("#tab-ranking").html(elx);

          const panelRight = L.control.sidepanel('panel-legend', {
              panelPosition: 'right',
              hasTabs: true,
              tabsPosition: 'left',
              pushControls: true,
              darkMode: true
          }).addTo(map);
        }

      </script>
