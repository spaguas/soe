        function loadUgrhiLayer(){
          let url = "https://datageo.ambiente.sp.gov.br/geoserver/datageo/ows?service=WFS&version=1.0.0&request=GetFeature&typeName=datageo:LimiteUgrhiDAEE&maxFeatures=50000&outputFormat=application%2Fjson";
          let filter_cql = {};

          if($("#ugrhi").val() != ""){
            filter_cql['cql_filter'] = "Codigo="+parseInt($("#ugrhi").val());

            $.ajax({
              url: url,
              cache: true,
              data: filter_cql,
              beforeSend: function(){
                ugrhiLayer.clearLayers();
              },
              success: function(data){
                console.log("Ugrhis: ", data);
                //localStorage.setItem('ugrhis_geo', JSON.stringify(data));

                var ll = L.geoJSON(data, {
                    filter: function(feature){
                      console.log("Ugrhi Properties: ", feature.properties);
                      if(feature != undefined && feature.properties != undefined){
                        return feature.properties.Codigo == parseInt($("#ugrhi").val());
                      }
                      else{
                        return false;
                      }
                    },
                    onEachFeature: function(feature,layer){
                      layer.on({
                        mouseover: highlightFeature,
                        mouseout: resetHighlight,
                        click: zoomToFeature
                      })
                    },
                    style: function (feature) {
                        return {color: '#aa66bb', opacity: 0.5};
                    }
                }).bindPopup(function (layer) {
                    return layer.feature.properties;
                });
                
                ugrhiLayer.addLayer(ll);
              },
              complete: function(){

              }
            });
          }
          else{
            console.log("Campo de Ugrhi não preenchido!")
          }

          
        }

